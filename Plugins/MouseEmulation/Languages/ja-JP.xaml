<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="meClick">クリック</system:String>
    <system:String x:Key="meMiddleClick">中クリック</system:String>
    <system:String x:Key="meRightClick">右クリック</system:String>

    <system:String x:Key="meLeft">左へマウスカーソルを移動</system:String>
    <system:String x:Key="meRight">右へマウスカーソルを移動</system:String>
    <system:String x:Key="meUp">上へマウスカーソルを移動</system:String>
    <system:String x:Key="meDown">下へマウスカーソルを移動</system:String>

    <system:String x:Key="meWheelUp">上方向ホイールスクロール</system:String>
    <system:String x:Key="meWheelDown">下方向ホイールスクロール</system:String>
    <system:String x:Key="meHWheelUp">左方向ホイールスクロール</system:String>
    <system:String x:Key="meHWheelDown">右方向ホイールスクロール</system:String>
    <system:String x:Key="meSpeedUp">カーソル移動速度を速くする</system:String>
    <system:String x:Key="meSlowDown">カーソル移動速度を遅くする</system:String>

</ResourceDictionary>