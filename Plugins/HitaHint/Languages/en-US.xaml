<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="hahClick">Click</system:String>
    <system:String x:Key="hahDoubleClick">Double click</system:String>
    <system:String x:Key="hahDragAndDrop">Drag and drop</system:String>
    <system:String x:Key="hahHover">Move the mouse pointer</system:String>
    <system:String x:Key="hahMiddleClick">Middle click</system:String>
    <system:String x:Key="hahMouseEmulation">Mouse emulation</system:String>
    <system:String x:Key="hahReload">Reload hints</system:String>
    <system:String x:Key="hahReverse">Reverse hints z-index</system:String>
    <system:String x:Key="hahRightClick">Right click</system:String>
    <system:String x:Key="hahShowInformation">Show control information</system:String>
    <system:String x:Key="hahSwitchKeyboardLayout">Switch keyboard layout</system:String>

    <system:String x:Key="hahHintkeys">Hint keys(used for a hint char)</system:String>
    <system:String x:Key="hahScreenWidthDivisionCount">Number of a screen partitions(used in screen division mode)</system:String>
    <system:String x:Key="hahHideMouseCursor">Automatically hide the mouse cursor</system:String>

</ResourceDictionary>