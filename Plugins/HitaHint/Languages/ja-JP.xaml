<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="hahClick">クリック</system:String>
    <system:String x:Key="hahDoubleClick">ダブルクリック</system:String>
    <system:String x:Key="hahDragAndDrop">ドラッグアンドドロップ</system:String>
    <system:String x:Key="hahHover">カーソルの移動</system:String>
    <system:String x:Key="hahMiddleClick">中クリック</system:String>
    <system:String x:Key="hahMouseEmulation">直接マウス操作(MouseEmulation)</system:String>
    <system:String x:Key="hahReload">ヒントの再作成</system:String>
    <system:String x:Key="hahReverse">ヒントの重なり順を逆転</system:String>
    <system:String x:Key="hahRightClick">右クリック</system:String>
    <system:String x:Key="hahShowInformation">コントロールの情報を表示</system:String>
    <system:String x:Key="hahSwitchKeyboardLayout">言語(キーボードレイアウト)の切替</system:String>

    <system:String x:Key="hahHintkeys">ヒントに使用する文字列</system:String>
    <system:String x:Key="hahScreenWidthDivisionCount">画面分割数(画面分割モード時に使用)</system:String>
    <system:String x:Key="hahHideMouseCursor">マウスを使用していないとき、マウスカーソルを隠す</system:String>

</ResourceDictionary>