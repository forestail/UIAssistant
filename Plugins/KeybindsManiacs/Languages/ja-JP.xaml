<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="kmMode">モード(_M)</system:String>
    <system:String x:Key="kmDefaultMode">デフォルトモード(_D)</system:String>
    <system:String x:Key="kmRunAtStartup">UIAssistant 起動時に実行する(_S)</system:String>
    <system:String x:Key="kmEnableDefaultKeybinds">Windows のキーバインドを有効にする</system:String>
    <system:String x:Key="kmPrefix">プレフィックスキー扱いにする</system:String>
    <system:String x:Key="kmSwapKey">キー入れ替え</system:String>
    <system:String x:Key="kmRunEmbeddedCommand">組み込みコマンド実行</system:String>
    <system:String x:Key="kmRunUIAssistantCommand">UIAssistant コマンド実行</system:String>
    <system:String x:Key="kmRunExtensionCommand">Extension 実行</system:String>

    <system:String x:Key="kmSwitchMode">{0} モードへ切り替えました</system:String>
    <system:String x:Key="kmActivate">Keybinds Maniacs を有効にしました</system:String>
    <system:String x:Key="kmDeactivate">Keybinds Maniacs を無効にしました</system:String>

</ResourceDictionary>