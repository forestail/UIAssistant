                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="kmMode">_Mode</system:String>
    <system:String x:Key="kmDefaultMode">_DefaultMode</system:String>
    <system:String x:Key="kmRunAtStartup">Run at UIAssistant _startup</system:String>
    <system:String x:Key="kmEnableDefaultKeybinds">Enable Windows default keybinds</system:String>
